from tipo_orcamento import TipoOrcamento

class Orcamento:
    def __init__(self, mes_ano: date, valor: float, tipo: TipoOrcamento):
        if isinstance(mes_ano, date):
            self.__mes_ano = mes_ano
        if isinstance(valor, float) or isinstance(valor, int):
            self.__valor = valor
        if isinstance(tipo, TipoOrcamento):
            self.__tipo = tipo

    @property
    def mes_ano(self) -> date:
        return self.__mes_ano

    @mes_ano.setter
    def mes_ano(self, mes_ano, date):
        if isinstance(mes_ano, date):
            self.__mes_ano = mes_ano

    @property
    def valor(self) -> valor:
        return self.__valor

    @valor.setter
    def valor(self, valor, float):
        if isinstance(valor, float) or isinstance(valor, int):
            self.__valor = valor

    @property
    def tipo(self) -> TipoOrcamento:
        return self.__tipo

    @tipo.setter
    def tipo(self, tipo, TipoOrcamento):
        if isinstance(tipo, TipoOrcamento):
            self.__tipo = tipo


    def adiciona_movimentacao(self, adiciona movimentacao)
